{
  "name": "DragonCodes",
  "version": "1.0.4",
  "repository": "",
  "description": "DragonCodes website",
  "config": {
    "buildDir": "./build",
    "stageWidth": 1920,
    "stageHeight": 1080
  },
  "scripts": {
    "startw": "SET NODE_ENV=development && npm run startg",
    "startg": "webpack-dev-server --content-base ./build/ --hot --inline --host 0.0.0.0 --port 3000",
    "start": "NODE_ENV=development && npm run startg",
    "webpack": "webpack -p --config ./webpack.config.js",
    "build": "NODE_ENV=production npm run webpack",
    "prewebpack": "npm run clean",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "lint": "eslint ./app/**/*.js",
    "reactman": "reactman --config ./templates/reactman.config.js"
  },
  "dependencies": {
    "eslint": "2.13.1",
    "glslify": "^6.0.1",
    "glslify-loader": "^1.0.2",
    "gsap": "^1.19.0",
    "page": "^1.7.1",
    "pixi.js": "^4.3.1",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "three": "^0.84.0",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.23.0",
    "babel-runtime": "^6.6.1",
    "brfs": "^1.4.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^2.13.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.8.4",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.26.0",
    "json-loader": "0.5.3",
    "pug": "^2.0.0-beta6",
    "pug-html-loader": "1.1.0",
    "pug-loader": "2.3.0",
    "reactman": "3.2.0",
    "rimraf": "2.4.3",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "style-loader": "0.13.1",
    "transform-loader": "0.2.3",
    "webpack": "2.3.2",
    "webpack-combine-loaders": "2.0.3",
    "webpack-dev-server": "2.4.2"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
